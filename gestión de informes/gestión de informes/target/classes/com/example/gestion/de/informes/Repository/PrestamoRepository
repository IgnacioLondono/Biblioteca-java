package com.example.gestion.de.informes.repository;

import com.example.GestionDeInformes.model.Prestamo;
import org.springframework.data.jpa.repository.JpaRepository;

import java.util.List;

public interface PrestamoRepository extends JpaRepository<Prestamo, Long> {
    List<Prestamo> findByFechaDevolucionIsNull();  // Método para obtener los préstamos activos
}
